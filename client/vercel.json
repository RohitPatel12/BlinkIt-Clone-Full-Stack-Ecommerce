{
  "version": 2,
  "projects": [
    {
      "name": "client",
      "source": "client/",
      "builds": [
        { "src": "client/package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
      ],
      "routes": [
        { "src": "/(.*)", "dest": "/index.html" }
      ]
    }
  ]
}
